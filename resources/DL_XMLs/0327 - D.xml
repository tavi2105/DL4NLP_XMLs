	<entry type="lemma">
		<form type="headword">
			<orth xml:lang="ro-x-accent-upcase-vowels">DROTAT</orth>    >
			<gramGrp>adj.</gramGrp>
			<usg>(Regional)</usg>
		</form>

		
		<sense class="0" value="DROTAT">
			<def>
				<MorfDef>adj.</MorfDef>
			</def>
			<sense class="1" value="1.">
				<def>
					<usg>(Despre păr)</usg>
					<RegDef>Ondulat (2)</RegDef>
					<dictScrap>
							<cit id="UNKNOWN">
							<quote><hi rend="italic">De sub căciulă îi năvăleau cârlionţii până deasupra sprâncenelor, ca un breton drotat.</hi></quote>
							<bibl>, Cf. DL<abbr type="siglum" author="TEODOREANU" pages="285" sigla="TEODOREANU, M." souce="M." volume="I">TEODOREANU, M. 1, 285</abbr></bibl>
							</cit>
					</dictScrap>
				</def>
				<sense class="2" value="2.">
					<def>
						<usg>(Despre obiecte de îmbrăcăminte sau părți ale acestora)</usg>
						<RegDef>Cu cute, cu onduleuri; plisat.</RegDef>
						<dictScrap>
							<cit id="UNKNOWN">
							<quote><hi rend="italic">Ea, o băbuşcă cu bărbia ascuţită sub care e înnodată panglica bonetei drotate ..., ține nişte mătănii de lemn de odogaci.</hi></quote>
							<bibl><abbr type="siglum" author="ANGHEL — IOSIF" pages="136" sigla="ANGHEL — IOSIF, C. L." souce="C. L.">ANGHEL — IOSIF, C. L. 136</abbr></bibl>
							</cit>
							<cit id="UNKNOWN">
							<quote><hi rend="italic">Îi potrivi guleraşul drotat al rochiei. </hi></quote>
							<bibl><abbr type="siglum" author="SADOVEANU" pages="465" sigla="SADOVEANU, O." souce="O." volume="XI">SADOVEANU, O. XI, 465</abbr></bibl>
							</cit>
							<cit id="UNKNOWN">
							<quote><hi rend="italic">Purza, ca toate celelalte slujnice, o bluză de pânză albă, cu guleraş şi mânicuţi drotate.</hi></quote>
							<bibl><abbr type="siglum" author="SADOVEANU" pages="419" sigla="SADOVEANU, O." souce="O." volume="XI">SADOVEANU, O. XI, 419</abbr></bibl>
							</cit>
						</dictScrap>
					</def>
				</sense>
			</sense>	
		</sense>
		<form type="varia">
			<form>- Pl.: <hi rend="italic"> drotați, -te.</hi></form>
			<form>- V. <hi rend="bold">drota</hi></form>
		</form>
	</entry>
	
	
	
