      <sense class="1" value="I.">
        <def>
          <MorfDef>s.f.</MorfDef>
        </def>
	  </sense>
        <sense class="2" value="1.">
          <def>
		  <RegDef><hi rend="bold">1.</hi>Femeie care face parte din populația de bază din părțile (de pe malul românesc al) Dunării sau este originară de acolo.</RegDef>
            <dictScrap>
			<cit id="UNKNOWN"><quote><hi rend="italic">Dunărencele sunt femei puternice.</hi></quote>.</cit>
			</dictScrap>
          </def>
		</sense>
			<sense class="3" value="◊">
			<def>
             <usg>(Adjectival)</usg>
              <dictScrap>
                <cit id="UNKNOWN"><quote><hi rend="italic">Femeile dunărence au costume specifice.</hi></quote>.</cit>
              </dictScrap>
			</def>
			</sense>
			<sense class="4" value="♦">
            <def>
			<usg>(De obicei art.)</usg>
			 <RegDef>Numele unui dans popular; melodie după care se execută acest dans</RegDef>
			 <dictScrap>
				<cit id="UNKNOWN"><quote><hi rend="italic">Jocurile călușarilor sunt: bătuta, chindia, dunăreanca, băltăreasca</hi></quote>
				<bibl><abbr type="siglum" author="PAMFILE" pages="69" sigla="PAMFILE, S.V." source="S.V">PAMFILE, S. V. 69, cf. VARONE, JOC.ROM.N.72, H IX 174</abbr></bibl>.</cit>
			  </dictScrap>
					</def>
			</sense>
			</sense>
